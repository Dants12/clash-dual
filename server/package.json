{
  "name": "clash-dual-server",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "tsc -p .",
    "pretest": "npm run build",
    "test": "node --test test/*.test.js && cross-env TS_NODE_TRANSPILE_ONLY=1 node --loader ts-node/esm --test test/*.test.ts",
    "start": "node dist/server.js",
    "dev": "node --watch dist/server.js"
  },
  "dependencies": {
    "decimal.js": "^10.6.0",
    "pino": "^9.11.0",
    "prom-client": "^15.1.3",
    "uuid": "^11.0.2",
    "ws": "^8.18.0",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@types/node": "^22.5.5",
    "@types/ws": "^8.5.12",
    "cross-env": "^10.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  },
  "ts-node": {
    "transpileOnly": true
  }
}
